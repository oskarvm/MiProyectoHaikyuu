<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/listaEquiposFragment"
        android:name="com.example.miproyectohaikyuu.ListaEquiposFragment"
        android:label="fragment_lista_equipos"
        tools:layout="@layout/fragment_lista_equipos" >
        <action
            android:id="@+id/action_listaEquiposFragment_to_listaPosicionesFragment"
            app:destination="@id/listaPosicionesFragment" />
        <action
            android:id="@+id/action_listaEquiposFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/iniciarSesionFragment"
        android:name="com.example.miproyectohaikyuu.IniciarSesionFragment"
        android:label="fragment_iniciar_sesion"
        tools:layout="@layout/fragment_iniciar_sesion" >
        <action
            android:id="@+id/action_iniciarSesionFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_iniciarSesionFragment_to_registroFragment"
            app:destination="@id/registroFragment" />
    </fragment>
    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.miproyectohaikyuu.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_iniciarSesionFragment"
            app:destination="@id/iniciarSesionFragment" />
    </fragment>
    <fragment
        android:id="@+id/cerrarSesionFragment"
        android:name="com.example.miproyectohaikyuu.CerrarSesionFragment"
        android:label="fragment_cerrar_sesion"
        tools:layout="@layout/fragment_cerrar_sesion" >
        <action
            android:id="@+id/action_cerrarSesionFragment_to_iniciarSesionFragment"
            app:destination="@id/iniciarSesionFragment" />
    </fragment>
    <fragment
        android:id="@+id/perfilFragment"
        android:name="com.example.miproyectohaikyuu.PerfilFragment"
        android:label="fragment_perfil"
        tools:layout="@layout/fragment_perfil" />
    <fragment
        android:id="@+id/registroFragment"
        android:name="com.example.miproyectohaikyuu.RegistroFragment"
        android:label="fragment_registro"
        tools:layout="@layout/fragment_registro" >
        <action
            android:id="@+id/action_registroFragment_to_iniciarSesionFragment"
            app:destination="@id/iniciarSesionFragment" />
        <action
            android:id="@+id/action_registroFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.miproyectohaikyuu.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_perfilFragment"
            app:destination="@id/perfilFragment" />
        <action
            android:id="@+id/action_homeFragment_to_cerrarSesionFragment"
            app:destination="@id/cerrarSesionFragment" />
        <action
            android:id="@+id/action_homeFragment_to_listaEquiposFragment"
            app:destination="@id/listaEquiposFragment" />
        <action
            android:id="@+id/action_homeFragment_to_misEquipos2"
            app:destination="@id/misEquipos2" />
        <action
            android:id="@+id/action_homeFragment_to_jugarPartida2"
            app:destination="@id/jugarPartida2" />
    </fragment>
    <fragment
        android:id="@+id/hinataFragment"
        android:name="com.example.miproyectohaikyuu.HinataFragment"
        android:label="fragment_hinata"
        tools:layout="@layout/fragment_hinata" />
    <fragment
        android:id="@+id/listaPosicionesFragment"
        android:name="com.example.miproyectohaikyuu.ListaPosicionesFragment"
        android:label="fragment_lista_posiciones"
        tools:layout="@layout/fragment_lista_posiciones" >
        <action
            android:id="@+id/action_listaPosicionesFragment_to_listaEquiposFragment"
            app:destination="@id/listaEquiposFragment" />
        <action
            android:id="@+id/action_listaPosicionesFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/misEquipos2"
        android:name="com.example.miproyectohaikyuu.misEquipos"
        android:label="fragment_mis_equipos"
        tools:layout="@layout/fragment_mis_equipos" >
        <action
            android:id="@+id/action_misEquipos2_to_crearEquipo"
            app:destination="@id/crearEquipo" />
        <action
            android:id="@+id/action_misEquipos2_to_equipoCreado"
            app:destination="@id/equipoCreado" />
    </fragment>
    <fragment
        android:id="@+id/jugarPartida2"
        android:name="com.example.miproyectohaikyuu.jugarPartida"
        android:label="fragment_jugar_partida"
        tools:layout="@layout/fragment_jugar_partida" />
    <fragment
        android:id="@+id/crearEquipo"
        android:name="com.example.miproyectohaikyuu.crearEquipo"
        android:label="fragment_crear_equipo"
        tools:layout="@layout/fragment_crear_equipo" />
    <fragment
        android:id="@+id/equipoCreado"
        android:name="com.example.miproyectohaikyuu.equipoCreado"
        android:label="fragment_equipo_creado"
        tools:layout="@layout/fragment_equipo_creado" />
</navigation>